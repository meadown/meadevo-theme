{
  "name": "Meadevo Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editorLineNumber.foreground": "#858585",
    "editorLineNumber.activeForeground": "#C6C6C6",
    "editorCursor.foreground": "#AEAFAD",
    "editor.selectionBackground": "#3A3A3A",
    "editor.inactiveSelectionBackground": "#3A3A3A66",
    "editor.lineHighlightBackground": "#28282800",
    "editorIndentGuide.activeBackground1": "#707070",
    "editorWhitespace.foreground": "#404040",

    "sideBar.background": "#1E1E1E",
    "sideBar.foreground": "#CCCCCC",
    "sideBarTitle.foreground": "#BBBBBB",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.foreground": "#CCCCCC",
    "sideBar.border": "#000000",

    "activityBar.background": "#181818",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#999999",
    "activityBar.border": "#00000000",
    "activityBar.activeBorder": "#007ACC",

    "statusBar.background": "#181818",
    "statusBar.foreground": "#CCCCCC",
    "statusBar.noFolderBackground": "#181818",
    "statusBar.border": "#00000000",

    "titleBar.activeBackground": "#1E1E1E",
    "titleBar.activeForeground": "#CCCCCC",
    "titleBar.inactiveBackground": "#1E1E1E",
    "titleBar.inactiveForeground": "#999999",
    "titleBar.border": "#00000000",

    "tab.activeBackground": "#1E1E1E",
    "tab.inactiveBackground": "#2D2D2D",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveForeground": "#999999",
    "tab.border": "#00000000",
    "tab.activeBorder": "#007ACC",
    "tab.activeBorderTop": "#00000000",
    "tab.hoverBackground": "#2A2A2A",

    "panel.background": "#1E1E1E",
    "panel.border": "#00000000",
    "panelTitle.activeForeground": "#E7E7E7",
    "panelTitle.inactiveForeground": "#999999",
    "panelTitle.activeBorder": "#007ACC",

    "terminal.background": "#1E1E1E",
    "terminal.foreground": "#D4D4D4",

    "list.activeSelectionBackground": "#3A3A3A",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.hoverBackground": "#2A2A2A",
    "list.inactiveSelectionBackground": "#37373D",
    "list.focusOutline": "#007ACC",

    "input.background": "#3C3C3C",
    "input.foreground": "#CCCCCC",
    "input.border": "#00000000",
    "input.placeholderForeground": "#A6A6A6",

    "button.background": "#007ACC",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#1A8FD8",

    "badge.background": "#007ACC",
    "badge.foreground": "#FFFFFF",

    "editorWidget.background": "#252526",
    "editorWidget.border": "#454545",
    "editorSuggestWidget.background": "#252526",
    "editorSuggestWidget.selectedBackground": "#3A3A3A",
    "editorHoverWidget.background": "#252526",
    "editorHoverWidget.border": "#454545",

    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.background": "#1e1e1e00",
    "scrollbarSlider.hoverBackground": "#2D2D2D",
    "scrollbarSlider.activeBackground": "#1E1E1E"
  },

  "tokenColors": [
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#7A8A50"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#9A8850"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#5A9088"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#A86B7A"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#6A8AB8"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6B7882",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#7A95B0"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.enummember",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#A87860"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#7A95B0"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#8A9A9A"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#A87860"
      }
    },
    {
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "#B86B68"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#8A9A9A"
      }
    }
  ]
}
